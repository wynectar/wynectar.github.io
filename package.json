{
  "name": "nectar-blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint --fix --ext .js,.ts,.vue src",
    "format": "prettier --write src/**/*.{js,ts,vue,json}"
  },
  "lint-staged": {
    "*.{js,ts}": ["eslint --fix", "prettier --write"],
    "*.css": ["stylelint --fix", "prettier --write"],
    "*.md": ["prettier --write"]
  },
  "dependencies": {
    "axios": "^1.9.0",
    "dompurify": "^3.2.6",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "marked": "^15.0.12",
    "marked-katex-extension": "^5.1.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^22.15.30",
    "@vicons/antd": "^0.13.0",
    "@vicons/fa": "^0.13.0",
    "@vicons/ionicons5": "^0.13.0",
    "@vicons/material": "^0.13.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "naive-ui": "^2.41.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.8"
  }
}
